  "description": "Answers to various LeetCode questions",
  "main": "index.js",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "find . -wholename '**/*.spec.js' -not -path './node_modules/*' | xargs -n1 node --experimental-modules",
    "test:watch": "npx chokidar-cli -i 'node_modules' '**/*.js' -c 'npm test'",
    "lint": "npx semistandard",
    "preversion": "npm run lint"
  },
  "dependencies": {},
  "devDependencies": {
    "tape": "^4.11.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "semistandard": {
    "ignore": [
      "index.js"
    ]
  }
}
